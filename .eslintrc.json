{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-restricted-syntax": 0,
        "no-await-in-loop": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-plusplus": 0,
        "max-len": [
            2,
            {
                "code": 120
            }
        ]
    }
}